/* Struct */
% suppose that there is a course separated into two classes
course(
    cse340,          
    time(1200, 1300),
    instructor(bryce, holton),
    coor300
).

course(
    cse340,
    time(1200, 1300),
    instructor(ivan, andersen),
    coor500
).

%
instructor(Instructtor, Class) :-
    course(Class,
           _,
           _, 
           Instructor,
           _).
